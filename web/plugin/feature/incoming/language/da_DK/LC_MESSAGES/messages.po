msgid "Scan incoming SMS for #groupcode"
msgstr "Søg i indkommende SMS efter #gruppekode"

msgid "Scan incoming SMS for @username"
msgstr "Søg i indkommende SMS efter @brugernavn"

msgid "Select users"
msgstr "Vælg brugere"

msgid "Send SMS to groups with found group codes in the incoming SMS"
msgstr "Send SMS til grupper når gruppekode findes i indkommende SMS"

msgid "Settings"
msgstr "Indstillinger"

msgid "keyword"
msgstr "nøgleord"
